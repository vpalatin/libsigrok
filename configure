  ($PKG_CONFIG --exists --print-errors "$sr_libusb_req") 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  	sr_have_libusb=yes
